#!/bin/sh

PLUGIN_VERSION=2.5.0-rc2

echo "install plugin jobsummary"

rm -rf jobsummary
cp ~/Projects/azkaban-plugins/dist/jobsummary/packages/azkaban-jobsummary-$PLUGIN_VERSION.tar.gz .
tar zxf azkaban-jobsummary-$PLUGIN_VERSION.tar.gz
mv azkaban-jobsummary-$PLUGIN_VERSION jobsummary
rm azkaban-jobsummary-$PLUGIN_VERSION.tar.gz

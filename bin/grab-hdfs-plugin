#!/bin/sh

PLUGIN_VERSION=2.5.0-rc3

echo "install plugin hdfs"

rm -rf hdfs
cp ~/Projects/azkaban-plugins/dist/hdfsviewer/packages/azkaban-hdfs-viewer-$PLUGIN_VERSION.tar.gz .
tar zxf azkaban-hdfs-viewer-$PLUGIN_VERSION.tar.gz
mv azkaban-hdfs-viewer-$PLUGIN_VERSION hdfs
rm azkaban-hdfs-viewer-$PLUGIN_VERSION.tar.gz

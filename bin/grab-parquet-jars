#!/bin/sh

VERSION="1.3.3-SNAPSHOT"
FORMAT_VERSION="2.0.0"

REPOSITORY_PATH="$HOME/.m2/repository/com/twitter"

cp $REPOSITORY_PATH/parquet-hadoop-bundle/$VERSION/parquet-hadoop-bundle-$VERSION.jar .
cp $REPOSITORY_PATH/parquet-pig-bundle/$VERSION/parquet-pig-bundle-$VERSION.jar .
cp $REPOSITORY_PATH/parquet-hive-bundle/$VERSION/parquet-hive-bundle-$VERSION.jar .
cp $REPOSITORY_PATH/parquet-avro/$VERSION/parquet-avro-$VERSION.jar .

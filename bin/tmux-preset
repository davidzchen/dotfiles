#!/bin/bash

tmux new-session -d -s 0
tmux new-window -c ~/                      -t 0:1 -n 'one'
tmux new-window -c ~/                      -t 0:2 -n 'two'
tmux new-window -c ~/                      -t 0:3 -n 'three'
tmux new-window -c ~/                      -t 0:4 -n 'four'
tmux new-window -c ~/                      -t 0:5 -n 'five'
tmux new-window -c ~/                      -t 0:6 -n 'six'
tmux new-window -c ~/                      -t 0:7 -n 'seven'
tmux select-window -t 0:1
tmux -2 attach-session -t 0

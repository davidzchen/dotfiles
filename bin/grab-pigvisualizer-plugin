#!/bin/sh

PLUGIN_VERSION=1.0

rm -rf pigvisualizer
cp ~/Projects/azkaban-plugins/dist/pigvisualizer/packages/azkaban-pigvisualizer-$PLUGIN_VERSION.tar.gz .
tar zxvf azkaban-pigvisualizer-$PLUGIN_VERSION.tar.gz
mv azkaban-pigvisualizer-$PLUGIN_VERSION pigvisualizer
rm azkaban-pigvisualizer-$PLUGIN_VERSION.tar.gz

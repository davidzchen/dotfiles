#!/bin/sh

PLUGIN_VERSION=2.5.0-rc2

echo "install plugin javaviewer"

rm -rf javaviewer
cp ~/Projects/azkaban-plugins/dist/javaviewer/packages/azkaban-javaviewer-$PLUGIN_VERSION.tar.gz .
tar zxf azkaban-javaviewer-$PLUGIN_VERSION.tar.gz
mv azkaban-javaviewer-$PLUGIN_VERSION javaviewer
rm azkaban-javaviewer-$PLUGIN_VERSION.tar.gz
